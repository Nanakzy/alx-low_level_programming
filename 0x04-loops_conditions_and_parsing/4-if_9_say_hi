#!/usr/bin/env bash
# This script displays "Best School" 10 times and says "Hi" on the 9th iteration

counter=0
while [ $counter -lt 10 ]; do
    if [ $counter -eq 9 ]; then
        echo "Hi"
    else
        echo "Best School"
    fi
    ((counter++))
done

# Print "Best School" one more time after the loop
echo "Best School"
